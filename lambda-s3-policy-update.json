{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "WriteCsvToParsedPrefix",
      "Effect": "Allow",
      "Action": [
        "s3:PutObject"
      ],
      "Resource": "arn:aws:s3:::vault-n8n/parsed/*"
    },
    {
      "Sid": "ReadMetadataFromIncoming",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject"
      ],
      "Resource": "arn:aws:s3:::vault-n8n/incoming/*"
    }
  ]
}
